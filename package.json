{
  "name": "@tkeel/console",
  "private": true,
  "homepage": "https://github.com/tkeel-io/console#readme",
  "bugs": {
    "url": "https://github.com/tkeel-io/console/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkeel-io/console.git"
  },
  "license": "Apache-2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "node ./scripts/run/build",
    "build-and-docker:build": "node ./scripts/run/build-and-docker-build",
    "build-and-docker:build-all": "node ./scripts/run/build-and-docker-build-all",
    "build:all": "node ./scripts/run/build-all",
    "check:packages": "node ./scripts/check/packages",
    "commit": "git-cz",
    "create:plugin": "node ./scripts/create-plugin && yarn && yarn init:all",
    "dev": "node ./scripts/run/dev",
    "dev:all": "node ./scripts/run/dev-all",
    "dev:portal-admin": "node ./scripts/run/dev-portal-admin",
    "dev:portal-tenant": "node ./scripts/run/dev-portal-tenant",
    "docker:build": "node ./scripts/run/docker-build",
    "docker:build-all": "node ./scripts/run/docker-build-all",
    "docker:push": "node ./scripts/docker/push",
    "docker:rm": "node ./scripts/docker/remove",
    "init:all": "yarn init:package-names && yarn init:root-tsconfig && yarn init:vscode-settings",
    "init:package-names": "node ./scripts/init/package-names",
    "init:root-tsconfig": "node ./scripts/init/root-tsconfig",
    "init:vscode-settings": "node ./scripts/init/vscode-settings",
    "postinstall": "husky install",
    "lint:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,hbs,handlebars,css,scss,md}\"",
    "lint:script": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:spell": "cspell --no-must-find-files \"**\"",
    "lint:style": "stylelint --fix \"**/*.{css,scss,js,jsx,ts,tsx}\"",
    "lint:typescript": "tsc --noEmit",
    "packages:set-all-versions": "node ./scripts/packages/set-all-versions",
    "pre-commit": "lint-staged",
    "show:all": "yarn show:base-paths && yarn show:dev-server-ports",
    "show:base-paths": "node ./scripts/show/base-paths",
    "show:dev-server-ports": "node ./scripts/show/dev-server-ports",
    "sort-package-json": "sort-package-json 'package.json' 'packages/*/package.json'"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
