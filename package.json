    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@svgr/webpack": "^6.2.1",
    "@types/axios": "^0.14.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tkeel-console": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
