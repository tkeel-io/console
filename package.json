{
  "name": "@tkeel/console",
  "private": true,
  "homepage": "https://github.com/tkeel-io/console#readme",
  "bugs": {
    "url": "https://github.com/tkeel-io/console/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkeel-io/console.git"
  },
  "license": "Apache-2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "init:all": "yarn init:root-tsconfig && yarn init:vscode-settings",
    "init:root-tsconfig": "node ./scripts/init-root-tsconfig",
    "init:vscode-settings": "node ./scripts/init-vscode-settings",
    "dev": "node ./scripts/run/dev",
    "dev:portal-admin": "node ./scripts/run/dev-portal-admin",
    "dev:portal-tenant": "node ./scripts/run/dev-portal-tenant",
    "dev:all": "node ./scripts/run/dev-all",
    "build": "node ./scripts/run/build",
    "build:all": "node ./scripts/run/build-all",
    "create:plugin": "node ./scripts/create-plugin && yarn && yarn init:all",
    "check:packages": "node ./scripts/check-packages",
    "sort-package-json": "sort-package-json 'package.json' 'packages/*/package.json'",
    "lint:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,hbs,handlebars,css,scss,md}\"",
    "lint:script": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:typescript": "tsc --noEmit",
    "lint:style": "stylelint --fix \"**/*.{css,scss,js,jsx,ts,tsx}\"",
    "lint:spell": "cspell --no-must-find-files \"**\"",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
